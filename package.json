{
  "name": "obsidian-veracrypt",
  "description": "Veracrypt for obsidian",
  "version": "0.3.204",
  "main": "index.js",
  "keywords": [],
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "clear": "rimraf dist node_modules",
    "setup0": "npm run clear && npm install --legacy-peer-deps && npm audit fix --force",
    "setup": "npm run clear && npm install --save && npm audit fix --force",
    "fix": "npm install --omit=dev  --no-audit --no-fund",
    "prebuild": "npm run bump",
    "bump": "standard-version",
    "commit": "git-cz",
    "deploy": "git add . && git commit -m '...' && git push",
    "format": "prettier src/ --write",
    "lint:fix": "eslint --ext .ts,.js,.json src/ --fix",
    "lint-staged": "lint-staged",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "build:prod": "obsidian-plugin build --with-stylesheet src/styles.css src/main.ts",
    "build:dev0": "obsidian-plugin dev --with-stylesheet src/styles.css src/main.ts --vault-path /pub/==vaults==/vera",
    "build:dev": "obsidian-plugin dev src/main.ts -v /pub/==vaults==/vera",
    "test": "jest",
    "start": "npm run dev",
    "dev": "npm run bump && make i && npm run build:dev"
  },
  "homepage": "https://github.com/valerian-borisovich/obsidian-veracrypt#readme",
  "author": {
    "name": "Valerian Borisovich",
    "email": "Valerian.Borisovich@gmail.com",
    "url": "https://valerian-borisovich.github.io/"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://github.com/valerian-borisovich"
  },
  "repository": {
    "type": "git",
    "url": "git+https://valerian-borisovich.github.io/obsidian-veracrypt"
  },
  "bugs": {
    "url": "https://github.com/valerian-borisovich/obsidian-veracrypt/issues"
  },
  "lint-staged": {
    "*.{js,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@remix-run/node": "^1.6.5",
    "@remix-run/react": "^1.6.5",
    "@types/node": "18.18.2",
    "axios": "latest",
    "cross-env": "^7.0.3",
    "dotenv": "latest",
    "electron": "^27.0.3",
    "fast-equals": "^4.0.1",
    "framer-motion": "^6.5.1",
    "fs-file-tree": "latest",
    "fs-jetpack": "latest",
    "glob": "latest",
    "gray-matter": "latest",
    "localforage": "latest",
    "mustache": "latest",
    "node-jq": "^4.2.2",
    "node-machine-id": "latest",
    "obsidian": "latest",
    "obsidian-plugin-cli": "latest",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.4.0",
    "uuid": "latest"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@electron-forge/cli": "latest",
    "@electron-forge/core": "latest",
    "@electron-forge/plugin-vite": "latest",
    "@electron-forge/plugin-webpack": "^7.1.0",
    "@remix-run/dev": "^1.6.5",
    "@remix-run/eslint-config": "^1.6.5",
    "@remix-run/serve": "^1.6.5",
    "@types/jest": "^28.1.6",
    "@types/mustache": "latest",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "ansi-colors": "^4.1.3",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^28.1.3",
    "builtin-modules": "latest",
    "commander": "latest",
    "commitizen": "^4.3.0",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dedent": "latest",
    "esbuild": "latest",
    "eslint": "^8.48.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsonc": "^2.9.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^28.1.3",
    "obsidian": "obsidianmd/obsidian-api",
    "obsidian-dataview": "latest",
    "obsidian-undocumented": "latest",
    "@types/obsidian-typings": "github:Fevol/obsidian-typings",
    "postcss": "^8.4.14",
    "prettier": "latest",
    "rimraf": "latest",
    "standard-version": "latest",
    "tailwindcss": "^3.1.6",
    "tslib": "latest",
    "typescript": "^4.9.5"
  },
  "config": {
    "files": [
      "main.js",
      "styles.css",
      "manifest.json",
      "vera.sh"
    ],
    "vault": {
      "name": "vera",
      "path": "/pub/==vaults==/vera",
      "conf": ".obsidian"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
