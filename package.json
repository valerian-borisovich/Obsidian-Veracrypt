{
  "name": "obsidian-veracrypt",
  "description": "Veracrypt for obsidian",
  "version": "0.3.64",
  "main": "index.js",
  "keywords": [],
  "license": "MIT",
  "type": "module",
  "scripts": {
    "clear": "rimraf dist node_modules",
    "setup0": "npm run clear && npm install --legacy-peer-deps && npm audit fix --force",
    "setup": "npm run clear && npm install --save && npm audit fix --force",
    "prebuild": "npm run bump",
    "bump": "standard-version",
    "commit": "git-cz",
    "deploy": "git add . && git commit -m '...' && git push",
    "format": "prettier src/ --write",
    "lint:fix": "eslint --ext .ts,.js,.json src/ --fix",
    "lint-staged": "lint-staged",
    "build:prod": "obsidian-plugin build --with-stylesheet src/styles.css src/main.ts",
    "build:dev0": "obsidian-plugin dev --with-stylesheet src/styles.css src/main.ts --vault-path /pub/==vaults==/vera",
    "build:dev": "obsidian-plugin dev src/main.ts -v /pub/==vaults==/vera",
    "test": "echo test pass...",
    "start": "npm run dev",
    "dev": "npm run bump && npm run build:dev"
  },
  "author": {
    "name": "Valerian Borisovich",
    "email": "Valerian.Borisovich@gmail.com",
    "url": "https://valerian-borisovich.github.io/"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://github.com/valerian-borisovich"
  },
  "repository": {
    "type": "git",
    "url": "git+https://valerian-borisovich.github.io/obsidian-veracrypt"
  },
  "bugs": {
    "url": "https://github.com/valerian-borisovich/obsidian-veracrypt/issues"
  },
  "lint-staged": {
    "*.{js,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@types/node": "latest",
    "@sindresorhus/slugify": "latest",
    "axios": "latest",
    "dotenv": "latest",
    "electron": "latest",
    "obsidian": "latest",
    "obsidian-plugin-cli": "latest",
    "node-machine-id": "latest",
    "node-jq": "latest",
    "fs-file-tree": "latest",
    "fs-jetpack": "latest",
    "glob": "latest",
    "gray-matter": "latest",
    "localforage": "latest",
    "uuid": "latest"
  },
  "devDependencies": {
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "ansi-colors": "^4.1.3",
    "bin-build": "^2.1.2",
    "builtin-modules": "latest",
    "commander": "latest",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dedent": "latest",
    "esbuild": "latest",
    "eslint": "^8.48.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsonc": "^2.9.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "obsidian": "obsidianmd/obsidian-api",
    "obsidian-dataview": "latest",
    "obsidian-undocumented": "latest",
    "prettier": "latest",
    "rimraf": "latest",
    "standard-version": "latest",
    "tslib": "latest",
    "typescript": "^4.5.4"
  },
  "config": {
    "vault": {
      "name": "vera",
      "path": "/pub/==vaults==/vera",
      "conf": ".obsidian"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
